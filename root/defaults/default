server {
	listen ${NGINX_PORT};
	location / {
		root /assets;
		try_files $uri $uri/ @github;
		autoindex on;
	}

	location @github {
		rewrite ^ /netbootxyz$uri break;

		proxy_buffer_size       128k;
		proxy_buffers           4 256k;
		proxy_busy_buffers_size 256k;

		proxy_pass https://github.com;
	}
}
